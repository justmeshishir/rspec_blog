<h1>Blog Posts</h1>
<% @posts.each do |post| %>
<div class="panel panel-body">
  <h2><%= post.title %> by <%= post.author %></h2>
  <p>
    <%= post.content %>
  </p>
  <%= link_to 'Edit', edit_post_path(id: post.id) %> | 
  <%= link_to 'Delete', post_path(id: post.id), method: :delete %>
</div>
<% end %>